{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "arrow-parens": "off",
      "no-underscore-dangle":"off",
      "no-unused-expressions":"off",
      "no-unused-vars": "off",
      "no-console": "off",
      "no-var": "off",
      "no-loop-func": "off",
      "no-cond-assign": "off",
      "no-return-assign": "off",
      "guard-for-in": "off",
      "no-dupe-keys": "off",
      "no-multi-assign": "off",
      "no-empty": "off",
      "default-case": "off",
      "eqeqeq": "off",
      "array-callback-return": "off",
      "operator-assignment": "off",
      "no-redeclare": "off",
      "no-use-before-define": "off",
      "no-restricted-syntax": "off",
      "no-constant-condition": "off",
      "no-mixed-operators": "off",
      "no-prototype-builtins": "off",
      "consistent-return": "off",
      "prefer-destructuring": "off",
      "prefer-const": "off",
      "vars-on-top": "off",
      "class-methods-use-this":"off",
      "camelcase":"off",
      "max-len": "off",
      "no-plusplus": "off",
      "no-tabs": "off",
      "no-continue": "off",
      "func-names": "off",
      "no-undef": "off",
      "new-cap": "off",
      "dot-notation": "off",
      "no-bitwise": "off",
      "no-restricted-properties": "off",
      "no-constant-return": "off",
      "block-scoped-var": "off",
      "no-restricted-globals": "off",
      "prefer-rest-params": "off",
      "prefer-spread": "off",
      "no-sequences": "off",
      "no-nested-ternary": "off",
      "no-useless-concat": "off",
      "no-void": "off",
      "no-array-constructor": "off",
      "brace-style": "off",  
      "max-classes-per-file": "off",
      "no-extend-native": "off",
      "no-labels": "off",
      "no-proto": "off",
      "no-new-func": "off",
      "no-new": "off",
      "implicit-arrow-linebreak": "off",
      "no-eval": "off",
      "radix": "off",
      "import/no-unresolved":"off"
    }
  }