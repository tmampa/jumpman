{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "arrow-parens": "off",
      "no-underscore-dangle":"off",
      "no-unused-expressions":"off",
      "no-unused-vars": "off",
      "no-console": "off",
      "class-methods-use-this":"off",
      "camelcase":"off",
      "max-len": "off",
      "no-plusplus": "off",
      "import/no-unresolved":"off"
    }
  }